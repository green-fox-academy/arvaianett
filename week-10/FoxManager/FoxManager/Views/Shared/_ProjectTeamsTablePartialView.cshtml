@model FoxManager.ViewModels.BaseViewModel

<h3>Project Teams:</h3>
<table class="table table-condensed">
    <tr>
        <th>Team</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var team in Model.TeamViewModel.AllTeams)
    {
        <tr>
            <td>@team.TeamName</td>
            <td>
                <a class="btn btn-default" href="/info/@Model.TeamViewModel.CurrentStudent.Name/@team.Id" role="button">
                    <span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span>
                </a>
            </td>
            <td>
                <a class="btn btn-default" href="/deleteteam/@Model.TeamViewModel.CurrentStudent.Name/@team.Id" role="button">
                    <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
                </a>
            </td>
        </tr>
    }
</table>

