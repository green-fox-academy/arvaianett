@model FoxManager.ViewModels.BaseViewModel

<form class="form-inline" action="/addteam/@Model.TeamViewModel.CurrentStudent.Name" method="post">
    <div class="form-group">
        <label class="sr-only">Add New Team</label>
        <input type="text" class="form-control" placeholder="Team" name="teamname">
    </div>
    <button type="submit" class="btn btn-default">Add</button>
</form>
