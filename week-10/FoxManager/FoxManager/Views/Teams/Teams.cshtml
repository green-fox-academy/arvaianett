@model System.Collections.Generic.List<FoxManager.Models.Team>
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-md-2"></div>
    <div class="row">
        <div class="col-md-4">
            <h3>Project Teams:</h3>
            <table class="table table-condensed">
                <tr>
                    <th>Team</th>
                </tr>
                @foreach (var team in Model)
            {
                    <tr>
                        <td>@team.TeamName</td>
                    </tr>
                }
            </table>
        </div>
        </div>
<div class="col-md-2"></div>

