@model PallidaRetake.ViewModels.WebshopViewModel

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Summary</title>
</head>
<body>
    <h1>Green Fox Fashion Web Store</h1>
    <h2>Summary</h2>
    <table>
        <tr>
            <th>Item name</th>
            <th>Manufacturer</th>
            <th>Category</th>
            <th>Quantity</th>
            <th>Sub-total price</th>
        </tr>
        <tr>
            <td>@Model.SelectedClothes.ItemName</td>
            <td>@Model.SelectedClothes.Manufacturer</td>
            <td>@Model.SelectedClothes.Catergory</td>
            <td>@Model.Quantity</td>
            <td>@Model.Price</td>
        </tr>
    </table>
    <a href="/warehouse">Back to main</a>
</body>
</html>
