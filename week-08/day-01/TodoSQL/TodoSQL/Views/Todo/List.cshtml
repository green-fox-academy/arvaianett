@model List<TodoSQL.Models.Todo>
<html>
<head>
    <title>Todo</title>
</head>
<body>
    <form action="/add" method="post">
        <input type="text" name="title" />
        <input type="text" name="isurgent" />
        <input type="text" name="isdone" />
        <input type="submit" value="submit" />
    </form>
    <h1>Todos</h1>
    <table>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Urgent</th>
            <th>Done</th>
        </tr>
        
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Title</td>
                    <td>@item.IsUrgent</td>
                    <td>@item.IsDone</td>
                </tr>
            }
        
    </table>
</body>
</html>