@model FoxClub.Models.Fox
<!DOCTYPE html>
<html>
<head>
    <title>Nutrition</title>
</head>
<body>
    <ul>
        <li><a href="/">Information</a></li>
        <li><a href="/nutrition">Nutrition Store</a></li>
        <li><a href="/tricks">Trick Center</a></li>
        <li><a href="/">Action History</a></li>
    </ul>
    <form action="changenutrition" method="post">
        <p>Food</p>
        <select type="text" name="food">
            <option value="apple">Apple</option>
            <option value="orange">Orange</option>
            <option value="grapes">Grapes</option>
            <option value="coconut">Coconut</option>
            <option value="raspberry">Raspberry</option>
        </select> 
        <p>Drink</p>
        <select type="text" name="drink">
            <option value="apple juice">Apple juice</option>
            <option value="orange juice">Orange juice</option>
            <option value="wine">Wine</option>
            <option value="coconut milk">Coconut milk</option>
            <option value="raspberry juice">Raspberry juice</option>
        </select>
        <input type="submit" value="Change" />
    </form>
</body>
</html>
