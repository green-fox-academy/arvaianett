@model FoxClub.Models.Fox
<!DOCTYPE html>
<html>
<head>
    <title>Fox Club Project</title>
</head>
<body>
    <ul>
        <li><a href="/">Information</a></li>
        <li><a href="/nutrition">Nutrition Store</a></li>
        <li><a href="/tricks">Trick Center</a></li>
        <li><a href="/">Action History</a></li>
    </ul>
    <img src="https://raw.githubusercontent.com/greenfox-academy/teaching-materials/master/project/programmer-fox-club/assets/greenfox.png?token=AWiat3UlJJnzdQwYsGuNVNQ5rKKrrAUpks5aY06ewA%3D%3D" alt="Fox">
    <p>This is @Model.Name Currently living on @Model.Food and @Model.Drink. He knows @Model.ListOfTricks.Count tricks.</p>
    <h1>Known tricks</h1>
    @if (Model.ListOfTricks.Count == 0)
    {
        <p>You know no tricks, yet. Go and learn some.</p>
    }
    else
    {
        <ul>
            @foreach (var item in Model.ListOfTricks)
            {
                <li>@item</li>
            }
        </ul>
    }
</body>
</html>
